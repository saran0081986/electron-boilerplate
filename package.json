{
  "name": "electron-boilerplate",
  "description": "Electron Boilerplate (based on Front Boilerplate 4.3.8)",
  "author": "Maxwellewxam <maxwellewxamofficial@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Maxwellewxam/electron-boilerplate.git"
  },
  "version": "1.3.3",
  "main": "build/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production node webpack/build.js",
    "clean": "del-cli build/* !build/empty tmp",
    "dev": "cross-env NODE_ENV=development node webpack/dev.js",
    "pack": "cross-env NODE_ENV=production node webpack/pack.js",
    "start": "electron .",
    "test": "cross-env NODE_ENV=test \"electron-mocha --opts mocha-main.opts ./tests/main && karma start karma.renderer.conf.js\"",
    "lint": "eslint --max-warnings 0 src/**/*.js tests/**/*.spec.js && stylelint src/**/*.css src/**/*.scss && pug-lint src && htmlhint src/**/*.html"
  },
  "devDependencies": {
    "autoprefixer": "^7.0.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.1.10",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.24.1",
    "bourbon": "^4.3.3",
    "chokidar": "^1.6.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.0",
    "css-mqpacker": "^6.0.0",
    "del-cli": "^0.2.1",
    "devtron": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-mocha": "^3.4.0",
    "electron-packager": "^8.5.2",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-loader": "^0.1.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "glob": "^7.1.1",
    "html-loader": "^0.4.5",
    "html-minifier-webpack-plugin": "^1.0.1",
    "htmlhint-loader": "^1.1.2",
    "htmlhint-stylish": "^1.0.3",
    "imagemin-webpack-plugin": "^1.4.4",
    "istanbul-instrumenter-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "^1.6.0",
    "karma-coverage-istanbul-reporter": "^1.0.0",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss-loader": "^2.0.0",
    "progress-bar-webpack-plugin": "^1.9.3",
    "pug": "^2.0.0-rc.1",
    "pug-html-loader": "^1.1.1",
    "pug-lint-loader": "^0.0.3",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.17.0",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-order": "^0.4.3",
    "stylelint-scss": "^1.4.3",
    "stylelint-webpack-plugin": "^0.7.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-hot-middleware": "^2.17.1",
